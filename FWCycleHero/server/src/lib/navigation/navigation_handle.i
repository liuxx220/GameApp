    float invSqrt = rsq(lenSquared);
    return Vector3(x * invSqrt, y * invSqrt, z * invSqrt);
}


inline void Vector3::normalise()
{
    float fLength = length();
	if (!(fLength < 0.00000001f && fLength > -0.00000001f))
	{
        float fInvLength = 1.f / fLength;
        x *= fInvLength;
        y *= fInvLength;
        z *= fInvLength;
	}
}


